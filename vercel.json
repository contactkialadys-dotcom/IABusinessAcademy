{
  "buildCommand": "pnpm install && rm -rf .expo node_modules/.cache && npx expo export --platform web",
  "outputDirectory": "dist",
  "devCommand": "pnpm dev:metro",
  "installCommand": "pnpm install",
  "framework": null,
  "ignoreCommand": "exit 0",
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ],
  "functions": {
    "**": {
      "maxDuration": 60
    }
  }
}
